{
  "extends": [
    "airbnb",
    "plugin:jest/recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "plugins": ["jest", "react", "prettier"],
  "rules": {
    "react/jsx-filename-extension": 0,
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true,
    "jest/globals": true
  }
}
